<server class="org.jpos.q2.iso.QServer" logger="Q2"
    name="simulator_10000">
  <attr name="port" type="java.lang.Integer">10000</attr>
  <attr name="minSessions" type="java.lang.Integer">100</attr>
  <attr name="maxSessions" type="java.lang.Integer">25000</attr>

  <!--
  <channel class="org.jpos.iso.channel.XMLChannel"
      logger="Q2" packager="org.jpos.iso.packager.XMLPackager">
  </channel>
  -->
  <channel class="org.jpos.iso.channel.ASCIIChannel" logger="Q2"
      packager="org.jpos.iso.packager.GenericPackager">
    <property name="packager-config" value="cfg/packager/dppkascii.xml" />
  </channel>

  <request-listener class="lab.aikibo.server.ServerListener" logger="Q2">
    <property name="space" value="tspace:mySpace" />
    <property name="queue" value="TransactionQueue" />
    <property name="timeout" value="10000" />
  </request-listener>
</server>
